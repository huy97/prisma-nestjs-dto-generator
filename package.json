    "prisma-generator",
    "prisma-nestjs-dto-generator",
    "nestjs dto",
    "nestjs dto swagger"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@prisma/generator-helper": "^4.0.0",
    "@prisma/internals": "^4.0.0",
    "class-validator": "^0.13.2",
    "pluralize": "^8.0.0",
    "prettier": "^2.7.1",
    "ts-morph": "^15.1.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "^2.6.3",
